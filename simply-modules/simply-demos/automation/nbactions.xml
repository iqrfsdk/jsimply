<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback/logback.xml -classpath %classpath com.microrisc.opengateway.OpenGateway</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback/logback.xml -classpath %classpath com.microrisc.opengateway.OpenGateway</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback/logback.xml -classpath %classpath com.microrisc.opengateway.OpenGateway</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir></exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-clean&amp;install</actionName>
            <displayName>clean&amp;install</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-install</actionName>
            <displayName>install</displayName>
            <goals>
                <goal>install</goal>
            </goals>
        </action>
    </actions>
